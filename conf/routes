# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~
GET     /                           com.mehmetakiftutuncu.quupnotifications.controllers.Application.index
POST    /login                      com.mehmetakiftutuncu.quupnotifications.controllers.QuupSessionController.login
POST    /logout/:registration       com.mehmetakiftutuncu.quupnotifications.controllers.QuupSessionController.logout(registration)
GET     /test/:session              com.mehmetakiftutuncu.quupnotifications.controllers.QuupSessionController.test(session)
